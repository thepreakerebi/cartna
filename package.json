{
  "name": "lemoncart",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "supermarket/backend",
    "supermarket/frontend",
    "customer/backend"
  ],
  "scripts": {
    "dev:supermarket-backend": "npm run dev -w @lemoncart/backend",
    "dev:supermarket-frontend": "npm run dev -w @lemoncart/frontend",
    "dev:customer-backend": "npm run dev -w @cartna/customer-backend",
    "build:backend": "npm run build -w @lemoncart/backend",
    "build:frontend": "npm run build -w @lemoncart/frontend",
    "start:backend": "npm run start -w @lemoncart/backend",
    "start:frontend": "npm run start -w @lemoncart/frontend",
    "start:customer": "npm run start -w @cartna/customer-backend"
  },
  "devDependencies": {
    "@types/validator": "^13.12.2"
  },
  "dependencies": {
    "firebase-admin": "^13.1.0"
  }
}
